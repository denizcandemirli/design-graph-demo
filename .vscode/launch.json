{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Streamlit (app.py)",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "app.py", "--server.headless=true"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
    }
  ]
}
